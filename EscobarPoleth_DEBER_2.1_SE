int i;
const int led[4]={13,12,11,10};
void setup(){
for(i=0;i<4;i++){
  pinMode(led[i],OUTPUT);
  }
}

void loop()
{
  for(i=0;i<4;i++){
  digitalWrite(led[i], HIGH);
  delay(100); 
  digitalWrite(led[i], LOW);
  delay(100);
  
  }
    for(i=3;i>0;i--){
  digitalWrite(led[i], HIGH);
  delay(100); 
  digitalWrite(led[i], LOW);
  delay(100);
  }
  
}
